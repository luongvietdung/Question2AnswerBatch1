<div class="qa-part-form-profile">
  <table class="qa-form-wide-table">
    <tbody id="avatar">
      <tr>
        <img src="https://image.flaticon.com/icons/svg/145/145852.svg" width="100" height="100" class="qa-avatar-image" alt="">
      </tr>
    </tbody>
    <tbody>
      <tr id="duration">
        <td class="qa-form-wide-label">
          Member for:
        </td>
        <td class="qa-form-wide-data">
          <span class="qa-form-wide-static">3000 day</span>
        </td>
      </tr>
      <tr id="level">
        <td class="qa-form-wide-label">
          Type:
        </td>
        <td class="qa-form-wide-data">
          <span class="qa-form-wide-static">Activated</span>
        </td>
      </tr>
      <tr id="userfield-5">
        <td class="qa-form-wide-label">
          Q2A site:
        </td>
        <td class="qa-form-wide-data">
          <span class="qa-form-wide-static"></span>
        </td>
      </tr>
      <tr id="userfield-1">
        <td class="qa-form-wide-label">
          Full name:
        </td>
        <td class="qa-form-wide-data">
          <span class="qa-form-wide-static"></span>
        </td>
      </tr>
      <tr id="userfield-1">
        <td class="qa-form-wide-label">
          Change password:
        </td>
        <td class="qa-form-wide-data">
          <span class="qa-form-wide-static">
            <%= link_to "Click here", edit_user_path(current_user)%>
          </span>
        </td>
      </tr>
    </tbody>
  </table>
</div>
